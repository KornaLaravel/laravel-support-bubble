{
    "name": "spatie/laravel-support-form",
    "description": "A non-intrusive support form that can be displayed on any page",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-support-form"
    ],
    "homepage": "https://github.com/spatie/laravel-support-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        },
        {
            "name": "Ruben Van Assche",
            "email": "ruben@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.15",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "spatie/pest-plugin-snapshots": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LaravelSupportForm\\": "src",
            "Spatie\\LaravelSupportForm\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LaravelSupportForm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "SupportFormServiceProvider"
            ],
            "aliases": {
                "LaravelSupportForm": "Spatie\\LaravelSupportForm\\LaravelSupportFormFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
